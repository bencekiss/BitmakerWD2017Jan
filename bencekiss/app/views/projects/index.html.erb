<h1>Projects#index</h1>
<p>
  <%= link_to "New Project", new_bence_project_path(Bence.first) %>
</p>
<div class="projects">
  <% @projects.each do |project| %>
    <div class="project">
      <div class="project-name">
        <%= project.name %>
      </div>
      <div class="project-url">
        <%= link_to "#{project.url}", project.url %>
      </div>
      <div class="project-description">
        <%= project.description %>
      </div>
    </div>
    <p>
      <%= link_to "Edit Project", edit_bence_project_path(Bence.first, project) %>
    </p>
  <% end %>

</div>
